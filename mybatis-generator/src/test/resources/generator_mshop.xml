		<table tableName="t_mobile_user"  domainObjectName="MobileUser" />
		<table tableName="t_recipient_addr"  domainObjectName="RecipientAddr" />
		<table tableName="t_admin_user"  domainObjectName="AdminUser" />
		<table tableName="t_user_login"  domainObjectName="UserLogin" />
		<table tableName="t_account"  domainObjectName="Account" />
		<table tableName="t_system_properties"  domainObjectName="SystemProperties" />
		<table tableName="t_sequence"  domainObjectName="Sequence" /> -->
		
		<!-- <table tableName="t_good_group_set"  domainObjectName="GoodGroupSet" />
		<table tableName="t_good_group"  domainObjectName="GoodGroup" />
